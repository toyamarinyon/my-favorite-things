{
  "name": "cloudflare-remix-sample2024",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix build",
    "dev": "remix dev --manual -c \"npm run start\"",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "wrangler pages dev --compatibility-date=2023-06-21 ./public",
    "typecheck": "tsc",
    "pages:deploy": "bun run build && wrangler pages deploy ./public",
    "generate:migration": "drizzle-kit generate:sqlite",
    "db:migrate": "wrangler d1 migrations apply DB --local"
  },
  "dependencies": {
    "@clerk/remix": "3.1.18",
    "@libsql/client": "0.4.3",
    "@paralleldrive/cuid2": "2.2.2",
    "@radix-ui/react-label": "2.0.2",
    "@radix-ui/react-radio-group": "^1.1.3",
    "@radix-ui/react-slot": "1.0.2",
    "@radix-ui/react-tooltip": "1.0.7",
    "@remix-run/cloudflare": "2.6.0",
    "@remix-run/cloudflare-pages": "2.6.0",
    "@remix-run/css-bundle": "2.6.0",
    "@remix-run/react": "2.6.0",
    "class-variance-authority": "0.7.0",
    "clsx": "2.1.0",
    "drizzle-orm": "0.29.3",
    "drizzle-valibot": "0.1.1",
    "framer-motion": "11.0.3",
    "install": "0.13.0",
    "isbot": "4.4.0",
    "lucide-react": "0.323.0",
    "openai": "4.27.0",
    "react": "18.2.0",
    "react-cropper": "2.3.3",
    "react-dom": "18.2.0",
    "tailwind-merge": "2.2.1",
    "tailwindcss-animate": "1.0.7",
    "tiny-invariant": "1.3.1",
    "ts-pattern": "5.0.6",
    "valibot": "0.28.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.5.3",
    "@cloudflare/workers-types": "v4.20240208.0",
    "@remix-run/dev": "2.6.0",
    "@types/react": "18.2.55",
    "@types/react-dom": "18.2.19",
    "drizzle-kit": "0.20.14",
    "tailwindcss": "latest",
    "typescript": "5.3.3",
    "wrangler": "3.28.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
